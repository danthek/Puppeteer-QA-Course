{
	"name": "puppeter_course",
	"version": "1.0.0",
	"description": "Automated software testing with puppeteer",
	"main": "index.js",
	"scripts": {
		"test:example": "node ./node_modules/mocha/bin/mocha.js --timeout=60000 ./tests/example.test.js",
		"test:device": "node ./node_modules/mocha/bin/mocha.js --timeout=60000 ./tests/device.test.js",
		"test:payment": "node ./node_modules/mocha/bin/mocha.js --timeout=60000 ./tests/payment.test.js",
		"test:e2e": "node ./node_modules/mocha/bin/mocha.js --timeout=60000 ./tests/e2e/*.test.js",
		"test:static": "node ./node_modules/mocha/bin/mocha.js --timeout=60000 ./tests/staticIncidents"
	},
	"keywords": [
		"Puppeteer",
		"Mocha",
		"Chai",
		"Automation"
	],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"chai": "^4.3.6",
		"jest": "^28.1.3",
		"jest-image-snapshot": "^5.1.0",
		"mocha": "^10.0.0",
		"prettier": "^2.7.1",
		"puppeteer": "^15.5.0"
	}
}
